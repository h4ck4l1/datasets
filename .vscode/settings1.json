{
    "workbench.colorCustomizations": {
        "editor.background": "#000000",
        "notebook.cellEditorBackground": "#262630",
        "editor.lineHighlightBackground": "#262630",
        "editor.lineHighlightBorder": "#262630",
        "notebook.editorBackground": "#202020"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "Victor Mono",
    "scm.inputFontFamily": "Victor Mono",
    "chat.editor.fontFamily": "Victor Mono",
    "debug.console.fontFamily": "Victor Mono",
    "editor.inlayHints.fontFamily": "Victor Mono",
    "terminal.integrated.fontFamily": "Victor Mono",
    "editor.inlineSuggest.fontFamily": "Victor Mono",
    "editor.fontSize": 21,
    "editor.fontLigatures": "'ss01'",
    "editor.codeLensFontFamily": "Victor Mono",
    "notebook.output.fontFamily": "Victor Mono",
    "markdown.preview.fontFamily": "Victor Mono",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "blink",
    "editor.cursorWidth": 1,
    "workbench.colorTheme": "One Dark Pro",
    "git.postCommitCommand": "sync",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "jupyter.askForKernelRestart": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.minimap.enabled": false,
    "warmUp.switchProgrammingLanguage": "python",
    "warmUp.changeCount": "60",
    "warmUp.changeTypingMode": "words (against the clock)",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.quickSuggestions": {
        "other": "off",
        "comments": "off",
        "strings": "off"
    },
    "editor.suggestOnTriggerCharacters": false,
    "editor.tabCompletion": "on",
    "editor.parameterHints.enabled": false
}
