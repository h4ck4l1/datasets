{
    "workbench.colorCustomizations": {
        "editor.background": "#000000",
        "notebook.cellEditorBackground": "#000000",
        "editor.lineHighlightBackground": "#000000",
        "editor.lineHighlightBorder": "#000000",
        "notebook.editorBackground": "#000000",
        "editorCursor.background": "#ffffff",
        "editorCursor.foreground": "#ffffff"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "Fira Code",
    "scm.inputFontFamily": "Fira Code",
    "chat.editor.fontFamily": "Fira Code",
    "debug.console.fontFamily": "Fira Code",
    "editor.inlayHints.fontFamily": "Fira Code",
    "terminal.integrated.fontFamily": "Fira Code",
    "editor.inlineSuggest.fontFamily": "Fira Code",
    "editor.fontSize": 30,
    "notebook.output.fontSize": 26,
    "notebook.markup.fontSize": 21,
    "editor.fontLigatures": "'calt', 'ss02', 'cv01', 'cv31', 'cv02', 'cv11', 'cv14', 'cv05', 'ss03', 'ss04', 'ss05', 'cv17', 'cv29', 'cv30', 'cv24', 'cv21', 'cv22', 'cv23', 'cv20', 'ss09', 'cv27', 'cv26', 'cv25', 'cv32', 'cv28', 'ss07', 'ss06'",
    "editor.codeLensFontFamily": "Fira Code",
    "notebook.output.fontFamily": "Fira Code",
    "markdown.preview.fontFamily": "Fira Code",
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "blink",
    "editor.cursorWidth": 4,
    "workbench.colorTheme": "One Dark Pro",
    "git.postCommitCommand": "sync",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "jupyter.askForKernelRestart": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.minimap.enabled": false,
    // "warmUp.switchProgrammingLanguage": "python",
    // "warmUp.changeCount": "120",
    // "warmUp.changeTypingMode": "words (against the clock)",
    // "editor.acceptSuggestionOnCommitCharacter": false,
    // "editor.quickSuggestions": {
    //     "other": "on",
    //     "comments": "on",
    //     "strings": "on"
    // },
    // "editor.suggestOnTriggerCharacters": false,
    "notebook.scrolling.experimental.anchorToFocusedCell": "on",
    "notebook.scrolling.revealNextCellOnExecute": "fullCell",
    "output.smartScroll.enabled": true,
    "notebook.stickyScroll.enabled": true,
    "notebook.stickyScroll.mode": "indented",
    "editor.stickyScroll.scrollWithEditor": true,
    "notebook.output.scrolling": true,
    // "editor.tabCompletion": "on",
    // "editor.parameterHints.enabled": false,
    "warmUp.switchNaturalLanguage": "english",
    "remoteHub.uncommittedChangesOnEntry": "none",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "right"
    }
}